{
  "name": "@arcticsnow/jupyterlab-julia-file",
  "version": "0.3.1",
  "description": "Create Julia Files from JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/arcticsnow/jupyterlab-julia-file",
  "bugs": {
    "url": "https://github.com/arcticsnow/jupyterlab-julia-file/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jeremy Tuloup, adapted for Julia by S. Filhol",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
